{
  "expo": {
    "name": "aesthetictodo",
    "slug": "aesthetictodo",
    "version": "1.0.01",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.kotka.aesthetictodo",
      "versionCode": 2
    },
    "description": "Dead simple ToDo app on React Native + Redux",
    "githubUrl": "https://github.com/xdebbie/aesthetictodo"
  }
}
